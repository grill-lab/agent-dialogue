<?xml version="1.0" encoding="UTF-8"?>
<web-app
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
        version="2.5">



    <display-name>Agent Dialogue Web Simulator</display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>resources/css/App.css</welcome-file>
        <welcome-file>resources/css/InputField.css</welcome-file>
    </welcome-file-list>



    <servlet>
        <servlet-name>JavaScriptLinker</servlet-name>
        <servlet-class>edu.gla.kail.ad.service.JavaScriptLinker</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JavaScriptLinker</servlet-name>
        <url-pattern>/java-script-linker</url-pattern>
    </servlet-mapping>
</web-app>